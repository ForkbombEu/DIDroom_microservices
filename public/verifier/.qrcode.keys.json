{
    "verifier_endpoint": "https://relying-party1.zenswarm.forkbomb.eu/verifier/generate_authorization_request",
    "object": {
	"response_mode":"direct_post",
	"response_type":"vp_token",
	"dcql_query":{
	    "credentials":[
		{
		    "id":"my_credential",
		    "format":"dc+sd-jwt",
		    "meta":{
			"vct_values":[
			    "discount_from_voucher"
			]
		    },
		    "claims":[
			{
			    "path":[
				"has_discount_from_voucher"
			    ],
			    "values":[
				"20",
				"30"
			    ]
			}
		    ]
		}
	    ]
	},
	"url":"https://relying-party1.zenswarm.forkbomb.eu/verifier"
    },
    "qrcode_path": "public/verifier/qrcode",
    "command": "./scripts/qrcode.sh"
}
